<div class="row" style="margin-bottom:7px !important;margin-top:7px !important">
    <div class="col-2" />
    <div class="col-2">
        <label>Member</label>
        <TelerikTextBox Id="name" @bind-Value="@LogBookCrewPassengerVM.Name" Placeholder="Name"></TelerikTextBox>
    </div>
    <div class="col-2">
        <label>Role</label>

        <TelerikComboBox Id=approach Data="@CrewPassengersList" TextField="Name"
                         Filterable="true" FilterOperator="@StringFilterOperator.Contains"
                         Placeholder="Select Role" ValueField="Name" @bind-Value="@(LogBookCrewPassengerVM.Role)">
        </TelerikComboBox>
    </div>
</div>