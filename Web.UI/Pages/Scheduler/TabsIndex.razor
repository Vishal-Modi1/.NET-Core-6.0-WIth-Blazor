        </HeaderTemplate>
        <Content>
            <Web.UI.Pages.Scheduler.SchedulerIndex></Web.UI.Pages.Scheduler.SchedulerIndex>
        </Content>
    </TabStripTab>

    <TabStripTab>
        <HeaderTemplate>
            <div class="d-flex justify-content-center align-items-center">

                <span class="mx-2">
                    <CustomIcons IconName="reservation" PrimaryColor="#3066f8"></CustomIcons>
                </span>
                <span>Reservations</span>
            </div>
        </HeaderTemplate>
