@page "/Scheduler"

<PageTitle> Scheduler | Fly Manager  </PageTitle>


<div id="container">
    <div class="card">

<TelerikTabStrip Class="k-tabstrip-list">
    
    <TabStripTab>
        <HeaderTemplate>
            <div class="d-flex justify-content-center align-items-center">
                <span class="mx-2">
                    <Web.UI.Shared.Components.CustomIcon.CustomIcons 
                    IconName="scheduler" PrimaryColor="#3066f8"></Web.UI.Shared.Components.CustomIcon.CustomIcons>
